     ,"INC-SOURCE":[
	{"x4pointer":" "
	,"x4codes":[{"code":"REAC", "dict":"INC-SOURCE", "idict":19,"hlp":"Reactor"}]
	,"x4freetext":[" Filtered neutrons with average energy 59 keV"
		,"scattered on hydrogen, included in the sample-scatterer"
		,"CH2, at scattering angles 15,20,25,30 degrees."
		,"The 59 keV filtered neutrons were extracted from the"
		,"reactor spectrum using an interference filter of the"
		,"following composition:S-116.53 g/cm2, Ni58-81.42 g/cm2,"
		,"V-24.44 g/cm2, Al-5.4 g/cm2, B10-0.517 g/cm2."
		,"Purity of the 59 keV filtered beam was about 95.2%,"
		,"other additions to the main spectra were for lines"
		,"47.8 keV-0.95%, 51.7 keV-1.72%, 349 keV-0.18%,690 keV-"
		,"0.11%. The limits of 95% response function for the"
		,"59 keV filter spectrum were defined as 53.3 to 61.4 keV"]
	}
      ]
     ,"INC-SPECT":[
	{"x4pointer":" "
	,"x4code":null
	,"x4freetext":["Neutron spectra obtained at the different scattering"
		,"angles had the following parameters:"
		,"at 15 deg.- Eav.=55 keV, Emin-48.6 keV,Emax-57.7 keV;"
		,"at 20 deg.- Eav.=52 keV, Emin-45.8 keV,Emax-55.3 keV;"
		,"at 25 deg.- Eav.=48.4 keV, Emin-42.5 keV,Emax-52.3 keV;"
		,"at 30 deg.- Eav.=44.2 keV, Emin-38.1 keV,Emax-48.6 keV."]
	}
      ]
     ,"METHOD":[
	{"x4pointer":" "
	,"x4codes":[{"code":"FNB", "dict":"METHOD", "idict":21,"hlp":"Filtered neutron beam"}
	    ,{"code":"TRN", "dict":"METHOD", "idict":21,"hlp":"Transmission method"}]
	,"x4freetext":["Transmission measurement of filtered neutrons."]
	}
      ]
