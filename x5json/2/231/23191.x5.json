     ,"DETECTOR":[
	{"x4pointer":" "
	,"x4codes":[{"code":"HE3SP", "dict":"DETECTOR", "idict":22,"hlp":"He-3 spectrometer"}]
	,"x4freetext":[" Matrix of polyethylene with 20 3He counters"
		,"arranged in two concentric rings around the detector"
		,"cavity. The detection of the neutrons is achieved via"
		,"the detection of the charged particles produced in the"
		,"reaction 3He + n -> 3H + p + 763.7 keV."
		,"According to MCNPX simulations, the neutron detection"
		,"efficiency 30% in the neutron energy range 1keV-1MeV."]
	}
	,{"x4pointer":" "
	,"x4codes":[{"code":"SISD", "dict":"DETECTOR", "idict":22,"hlp":"Silicon strip detector"}]
	,"x4freetext":[" Si detector (27 mm diameter and 910 microm thick"
		,"to detect beta-particles."]
	}
	,{"x4pointer":" "
	,"x4codes":[{"code":"GE", "dict":"DETECTOR", "idict":22,"hlp":"Germanium detector"}]
	,"x4freetext":["  Ge detector for  gamma-ray detection."
		,"BELEN-20 detection efficiency obtained using 88Br and 9"
		,"as calibration:"
		,"Isotope    Pn(%)       Efficiency"
		,"88Br       6.58+-0.18  27.6+-0.7"
		,"95Rb       8.73+-0.20  26.6+-0.8"
		,"Average efficiency for BELEN-20 is (27.1+-0.8)%."]
	}
      ]
