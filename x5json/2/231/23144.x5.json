     ,"INC-SOURCE":[
	{"x4pointer":" "
	,"x4codes":[{"code":"P-LI7", "dict":"INC-SOURCE", "idict":19,"hlp":"7Li(p,n)"}]
	}
      ]
     ,"INC-SPECT":[
	{"x4pointer":" "
	,"x4code":null
	,"x4freetext":["Neutron spectra measured by TOF at 0 and 30 degrees"
		,"respective to the proton beam are on Fig.2 of"
		,"J,NSTP,1,89,2011."
		,"Quasi-monoenergetic neutron spectrum was obtained by"
		,"subtracting the spectrum at 30 degrees from the"
		,"spectrum at 0 degrees."]
	}
      ]
     ,"DETECTOR":[
	{"x4pointer":"1"
	,"x4codes":[{"code":"SCIN", "dict":"DETECTOR", "idict":22,"hlp":"Scintillation detector"}]
	,"x4freetext":[" Intensity and energy spectrum of source neutrons"
		,"produced from the 7Li(p,n) reaction were measured by an"
		,"NE213 liquid scintillator placed at 17.10 or 16.13 m"
		,"Li-target at 0 or 30 degrees, respectively."]
	}
	,{"x4pointer":"2"
	,"x4codes":[{"code":"HPGE", "dict":"DETECTOR", "idict":22,"hlp":"Hyperpure Germanium detector"}]
	,"x4freetext":[" HPGe detector for gamma rays measurement"]
	}
      ]
     ,"PART-DET":[
	{"x4pointer":"1"
	,"x4codes":["N"]
	}
	,{"x4pointer":"2"
	,"x4codes":["G"]
	}
      ]
     ,"SAMPLE":[
	{"x4pointer":" "
	,"x4code":null
	,"x4freetext":["Samples were placed at 8.05 or 7.05 m from the lithium"
		,"target at 0 or 30 degrees, were irradiated 26 hr or"
		,"18 hr, respectively."]
	}
      ]
     ,"METHOD":[
	{"x4pointer":"1"
	,"x4codes":[{"code":"TOF", "dict":"METHOD", "idict":21,"hlp":"Time-of-flight"}]
	,"x4freetext":[" For neutron beam."]
	}
	,{"x4pointer":"2"
	,"x4codes":[{"code":"GSPEC", "dict":"METHOD", "idict":21,"hlp":"Gamma ray spectrometry"}]
	,"x4freetext":[" Gamma-ray spectrometry of irradiated samples."]
	}
	,{"x4pointer":" "
	,"x4codes":[{"code":"ACTIV", "dict":"METHOD", "idict":21,"hlp":"Activation"}]
	,"x4freetext":[" Activation."]
	}
      ]
     ,"ANALYSIS":[
	{"x4pointer":" "
	,"x4code":null
	,"x4freetext":["Activation reaction rates R were estimated by the peak"
		,"counts of gamma-ray spectra, the peak efficiency of"
		,"HPGe detector (calculated by EGS4 code), the self-"
		,"absorption of gamma-rays in samples, the beam current"
		,"fluctuation during irradiation."
		,"Cross-sections were estimates as:"
		,"SIG=(R(0deg)- f*R(30deg))/F, where"
		,"f- is correction factor,"
		,"F- neutron fluence rate per beam current."]
	}
      ]
     ,"CORRECTION":[
	{"x4pointer":" "
	,"x4code":null
	,"x4freetext":["For self-absorption in sample by EGS4 code."
		,"f - correction in subtraction of neutron spectrum due"
		,"to variation of distances :"
		,"- 17.10 to 16.13m between Li-target and NE213,"
		,"- 8.05 to 7.05m between Li-target and samples."]
	}
      ]
