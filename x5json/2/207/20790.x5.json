	}
      ]
     ,"INC-SOURCE":[
	{"x4pointer":" "
	,"x4codes":[{"code":"D-BE", "dict":"INC-SOURCE", "idict":19,"hlp":"9Be(d,n)"}]
	,"x4freetext":[" NPL STANDARD THERMAL NEUTRON SOURCE"
		," DESCRIBED IN DETAIL IN JNE 22 759 1968"
		," FLUX LEVEL 1.26X10(7) N/CM*2/S. THIS VALUE"
		," IS MEASURED TO 0.6 PERCENT RELATIVE TO THE"
		," GOLD CAPTURE CROSS SECTION."]
	}
      ]
     ,"INC-SPECT":[
	{"x4pointer":" "
	,"x4code":null
	,"x4freetext":["        NPL STANDARD MAXWELLIAN THERMAL SPECTRUM."]
	}
      ]
     ,"SAMPLE":[
	{"x4pointer":" "
	,"x4code":null
	,"x4freetext":[".ALL SAMPLES WERE THIN DISCS OF 22MM DIAMETER,"
		," ENCLOSED IN THIN PAPER COVERS."]
	}
      ]
     ,"METHOD":[
	{"x4pointer":" "
	,"x4codes":[{"code":"ACTIV", "dict":"METHOD", "idict":21,"hlp":"Activation"}]
	,"x4freetext":[" ACTIVITY MEASURED BY 4PI BETA-GAMMA"
		," COINCIDENCE COUNTING."]
	}
      ]
     ,"DETECTOR":[
	{"x4pointer":" "
	,"x4codes":[{"code":"PROPC", "dict":"DETECTOR", "idict":22,"hlp":"Proportional counter"}]
