     ,"TITLE":[
	{"x4pointer":" "
	,"x4code":null
	,"x4freetext":["-TERNARY FISSION OF U-233,U-235,NP-237 AND"
		," PU-239 INDUCED BY THERMAL NEUTRONS.-"
		," (IN DUTCH) -"]
	}
      ]
     ,"FACILITY":[
	{"x4pointer":" "
	,"x4codes":[{"code":"REAC", "dict":"FACILITY", "idict":18,"hlp":"Reactor"}
	    ,{"code":"2FR ILL", "dict":"INSTITUTE", "idict":3,"hlp":"Institut Laue-Langevin, Grenoble, France"}]
	,"x4freetext":[" 87 M CURVED NEUTRON GUIDE OF THE"
		," HIGH FLUX REACTOR. FLUX 1.E+08 N/CM**2/SEC,"
		," CADMIUM RATIO 1.0E+6, GAMMA BACKGROUND REDUCED"
		," BY A FACTOR OF 1.0E+06."]
	}
      ]
     ,"INC-SOURCE":[
	{"x4pointer":" "
	,"x4codes":[{"code":"REAC", "dict":"INC-SOURCE", "idict":19,"hlp":"Reactor"}]
	}
      ]
     ,"INC-SPECT":[
	{"x4pointer":" "
	,"x4code":null
	,"x4freetext":["THERMAL MAXWELLIAN SPECTRUM FROM HFR."]
	}
      ]
     ,"METHOD":[
	{"x4pointer":" "
	,"x4codes":[{"code":"EDE", "dict":"METHOD", "idict":21,"hlp":"Particle identification by `E/Delta E` measurement"}]
	,"x4freetext":[" MEASUREMENT OF BINARY AND TERNARY"
		," EVENTS BY A TELESCOPE DETECTOR WITHOUT"
		," USING A COINCIDENCE."]
	}
      ]
     ,"DETECTOR":[
	{"x4pointer":" "
	,"x4codes":[{"code":"TELES", "dict":"DETECTOR", "idict":22,"hlp":"Counter telescope"}
	    ,{"code":"SOLST", "dict":"DETECTOR", "idict":22,"hlp":"Solid-state detector"}
	    ,{"code":"SOLST", "dict":"DETECTOR", "idict":22,"hlp":"Solid-state detector"}]
	,"x4freetext":[" TELESCOPE CONSISTING OF"
		," AN E- AND A DE- DETECTOR. THE DE-DETECTOR"
		," WAS A SI(AU) SURFACE BARRIER WITH A DEPLETED"
		," ZONE OF 26.7 MICRONS THICKNESS, A SURFACE"
		," OF 300. MM**2 AND A RESOLUTION FWHM FOR"
		," THE 5.486 MEV ALPHA-LINE OF AM-241 OF 35"
		," KEV. THE E-DETECTOR HAD A DEPLETED ZONE OF"
		," 500 MICRONS, A SURFACE OF 450 MM**2, AND"
		," A RESOLUTION OF 40. KEV. IT WAS ALSO A"
		," SI(AU) SURFACE BARRIER DETECTOR."]
	}
      ]
     ,"COMMENT":[
	{"x4pointer":" "
	,"x4code":null
	,"x4freetext":[".WITH THIS THICKNESS OF THE DE-DETECTOR THE"
		," SIGNAL FOR THE Z=1 TERNARY PARTICLES IS"
		," COMPLETELY DIFFERENT FROM THE ONE FOR THE"
		," ALPHA PARTICLES WHICH ALLOWS THUS STILL"
		," PARTICLE IDENTIFICATION."
		,".DIFFERENCES WITH FORMERLY REPORTED VALUES"
		," ARE EXPLAINED BY THE EXTRAPOLATION TO"
		," THE LOWER ENERGIES OF THE ALPHA PARTICLE"
		," SPECTRA WHICH DEVIATE FROM A GAUSSIAN."]
	}
      ]
     ,"STATUS":[
	{"x4pointer":" "
	,"x4code":null
	,"x4freetext":[".DATA TAKEN FROM MAIN REFERENCE."]
	}
      ]
