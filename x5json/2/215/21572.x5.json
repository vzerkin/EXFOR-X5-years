     ,"SAMPLE":[
	{"x4pointer":" "
	,"x4code":null
	,"x4freetext":[".243-AM OXIDE (30.60+-0.15 MICROGRAMS) AND NATURAL"
		," URANIUM OXIDE (44.20+-0.88 MICROGRAMS). THE MASS"
		," ANALYSIS FOR THE AM MATERIAL WAS - 99.969 WT PER"
		," CENT FOR AM-243, 0.031 WT PER CENT FOR AM-241 AND"
		," LESS THAN 0.00003 WT PER CENT FOR AM-242 AND AM-244."]
	}
      ]
     ,"METHOD":[
	{"x4pointer":" "
	,"x4code":null
	,"x4freetext":[".THE FISSION FRAGMENTS WERE RECORDED WITH A MULTI-8"
		," DATA RECORDING SYSTEM."]
	}
      ]
     ,"DETECTOR":[
	{"x4pointer":" "
	,"x4codes":[{"code":"FISCH", "dict":"DETECTOR", "idict":22,"hlp":"Fission chamber"}]
	,"x4freetext":[" FISSION FRAGMENTS WERE DETECTED BY TWO"
		," SURFACE BARRIER DETECTORS."]
	}
      ]
     ,"ANALYSIS":[
	{"x4pointer":" "
	,"x4code":null
	,"x4freetext":[".70000 FISSIONS PER DAY FOR AM-243 TARGETS. THE AM-"
		," 243 MAXWELLIAN CROSS SECTION WAS CALCULATED BY"
		," COMPARISON OF THE CORRESPONDING FISSION EVENTS OF"
		," AM-243 AND NATURAL URANIUM TARGETS."]
	}
