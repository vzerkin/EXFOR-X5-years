     ,"SAMPLE":[
	{"x4pointer":" "
	,"x4code":null
	,"x4freetext":[".U-235 TARGET PREPARED AT CBNM, GEEL, BY EVAPORATION"
		," OF 97.663 AT. PERC. ENRICHED U-F(4) ON A 10 MICRONS"
		," THICK AL-FOIL. THE THICKNESS OF THIS LAYER IS 3.6"
		," MICROGRAMS/CM*CM. THE COMBINATION OF SUCH A THIN"
		," TARGET AND AN INTENSE NEUTRON BEAM AMELIORATES THE"
		," SIGNAL (I.E. NEUTRON INDUCED ALPHAS) TO BACKGROUND"
		," (I.E. SPONTANEOUS DECAY ALPHAS). BESIDES, THE ENERGY"
		," LOSS OF THE ALPHA PARTICLES WITHIN THE TARGET IS"
		," NEGLIGIBLE."]
	}
      ]
     ,"METHOD":[
	{"x4pointer":" "
	,"x4codes":[{"code":"EDE", "dict":"METHOD", "idict":21,"hlp":"Particle identification by `E/Delta E` measurement"}]
	,"x4freetext":[" ALPHA PARTICLES IDENTIFIED BY E-DE DETECTOR."]
	}
      ]
     ,"DETECTOR":[
	{"x4pointer":" "
	,"x4codes":[{"code":"TELES", "dict":"DETECTOR", "idict":22,"hlp":"Counter telescope"}
