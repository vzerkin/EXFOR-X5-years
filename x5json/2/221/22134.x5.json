     ,"TITLE":[
	{"x4pointer":" "
	,"x4code":null
	,"x4freetext":["-NUCLEAR CHARGE DISTRIBUTION IN THE REACTOR NEUTRON"
		," INDUCED FISSION OF 232-TH, FRACTIONAL CUMULATIVE"
		," YIELDS OF THE ISOTOPES OF KRYPTON AND XENON."]
	}
      ]
     ,"AUTHOR":[
	{"x4pointer":" "
	,"x4codes":[{"ini":"A.","nam":"SRIVASTAVA"}
	    ,{"ini":"H.O.","nam":"DENSCHLAG"}]
	}
      ]
     ,"INSTITUTE":[
	{"x4pointer":" "
	,"x4codes":[{"code":"2GERMNZ", "dict":"INSTITUTE", "idict":3,"hlp":"Johannes Gutenberg-Universitaet Mainz, Mainz, Germany"}]
	}
      ]
     ,"FACILITY":[
	{"x4pointer":" "
	,"x4codes":[{"code":"REAC", "dict":"FACILITY", "idict":18,"hlp":"Reactor"}]
	,"x4freetext":[" THE MAINZ TRIGA REACTOR WAS USED FOR THE"
		," IRRADIATIONS."]
	}
      ]
     ,"INC-SOURCE":[
	{"x4pointer":" "
	,"x4codes":[{"code":"REAC", "dict":"INC-SOURCE", "idict":19,"hlp":"Reactor"}]
	,"x4freetext":[" THE IRRADIATION POSITION WAS IN THE CENTER OF"
		," THE REACTOR, WHERE THE FAST, EPI-CADMIUM NEUTRON FLUX"
		," IS OF THE ORDER OF 1.0E+11 NEUTRONS/CM2 AND SEC."]
	}
      ]
     ,"INC-SPECT":[
	{"x4pointer":" "
	,"x4code":null
	,"x4freetext":[".FAST REACTOR SPECTRUM WITH A CADMIUM RATIO OF ABOUT"
		," 30."]
	}
      ]
     ,"METHOD":[
	{"x4pointer":" "
	,"x4codes":[{"code":"OLMS", "dict":"METHOD", "idict":21,"hlp":"On-line mass separation of a product"}]
	,"x4freetext":[" THE FISSION FRAGMENTS RECOILING OUT OF THE"
		," SAMPLE FOIL WERE STOPPED IN A LAYER OF MAGNESIUM"
		," STEARATE FROM WHICH ONLY THE RARE GASES DIFFUSED INTO"
		," AN ADJACENT EVACUATED SPACE WHERE THEY DECAYED TO"
		," LONGER LIVED DESCENDENTS THAT DEPOSITED ON THE WALLS."]
	}
	,{"x4pointer":" "
	,"x4codes":[{"code":"ABSFY", "dict":"METHOD", "idict":21,"hlp":"Absolute fission yield measurement"}]
	,"x4freetext":[" BY MEASURING THE ACTIVITY OF THE DESCENDENTS,"
		," THE FRACTIONAL CUMULATIVE YIELD COULD BE DETERMINED"
		," DIRECTLY."]
	}
      ]
     ,"DETECTOR":[
	{"x4pointer":" "
	,"x4codes":[{"code":"GE-IN", "dict":"DETECTOR", "idict":22,"hlp":"Germanium intrinsic detector"}]
	,"x4freetext":[" AFTER RADIOCHEMICAL SEPARATION OF THE DESCEN-"
		," DENTS IN THE STEARATE AND ON THE WALLS, THE ACTIVITY"
		," OF THE SAMPLES WERE COUNTED IN A FIXED GEOMETRY ON A"
		," 154 CM3 INTRINSIC GERMANIUM DETECTOR, HAVING A RESO-"
		," LUTION OF 1.8 KEV AT 1332 KEV."]
	}
      ]
     ,"SAMPLE":[
	{"x4pointer":" "
	,"x4code":null
	,"x4freetext":[".THE SAMPLE WAS A 232-TH METAL FOIL WITH A THICKNESS OF"
		," 10 MICRO-M AND A WEIGHT OF 1 MG. THE SAMPLE WAS MOUNT-"
		," ED IN AN IRRADIATION SETUP MADE OF POLYETHYLENE IN"
		," ORDER TO MINIMIZE THE DOSE FROM NEUTRON ACTIVATION."
		," THE SAMPLE WAS IRRADIATED FOR 60 MIN."]
	}
      ]
     ,"CORRECTION":[
	{"x4pointer":" "
	,"x4code":null
	,"x4freetext":[".THE ACTIVITIES WERE CORRECTED FOR CHEMICAL YIELD, FOR"
		," THE GAS VOLUME BETWEEN THE TH-TARGET AND STEARATE, AND"
		," FOR THE NON NEGLIGIBLE GAS VOLUME WITHIN THE STEARATE"
		," GRAINS."]
	}
      ]
