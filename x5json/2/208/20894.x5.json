     ,"TITLE":[
	{"x4pointer":" "
	,"x4code":null
	,"x4freetext":["-MEASUREMENT OF THE LI-6(N,ABSORPTION) AND B-10(N,"
		," ABSORPTION) CROSS SECTIONS BY THE SHELL TRANSMISSION"
		," METHOD-"]
	}
      ]
     ,"FACILITY":[
	{"x4pointer":" "
	,"x4codes":[{"code":"VDG", "dict":"FACILITY", "idict":18,"hlp":"Van de Graaff"}]
	,"x4freetext":[" 3 MEV VAN DER GRAAF."]
	}
      ]
     ,"INC-SOURCE":[
	{"x4pointer":" "
	,"x4codes":[{"code":"P-LI7", "dict":"INC-SOURCE", "idict":19,"hlp":"7Li(p,n)"}]
	,"x4freetext":[" LI-7(P,N) REACTION USED AS A NEUTRON SOURCE"
		,"        OUTSIDE THE SPHERE."
		,".V-51(P,N) USED AS THE SOURCE WHEN INSIDE THE SPHERE."
		,"        BOTH METHODS WERE USED TO REDUCE THE CHANCE"
		,"        OF SYSTEMATIC ERRORS IN THE CORRECTIONS FOR"
		,"        ANY ONE METHOD AFFECTING THE RESULTS."]
	}
      ]
     ,"METHOD":[
	{"x4pointer":" "
	,"x4code":null
	,"x4freetext":[".THE SPHERICAL SHELL TRANSMISSION METHOD CHOSEN"
		," AS THE MEASUREMENT IS THEN INDEPENDENT OF ANY"
		," KNOWLEDGE OF THE ABSOLUTE NEUTRON FLUX."]
	}
      ]
     ,"DETECTOR":[
	{"x4pointer":" "
	,"x4codes":[{"code":"BF3", "dict":"DETECTOR", "idict":22,"hlp":"Boron Trifluoride neutron detector"}]
	,"x4freetext":[".FOR MEASUREMENTS USING THE LI-7 SOURCE, I.E DETECTOR"
		," INSIDE THE SAMPLE SHELL, A CADMIUM COVERED"
		," BF-3 COUNTER 4 CM IN DIAMETER WAS USED AT 1 ATM."
		,".USING THE V-51 SOURCE, I.E. DETECTORS OUTSIDE THE"
		," SHELL, SEVEN PARAFFIN WAX MODERATED BF-3 COUNTERS"
		," WERE USED TO INTEGRATE OVER NEUTRON ANGULAR DISTR-"
		," IBUTIONS. THESE COUNTERS WERE 2.5 CM IN DIAMETER"
		," AND 12 CM LONG."]
	}
      ]
     ,"PART-DET":[
	{"x4pointer":" "
	,"x4codes":["N"]
	,"x4freetext":[" NEUTRONS THAT HAVE PENETRATED THE SAMPLE"
		,"        SPHERE."]
	}
      ]
