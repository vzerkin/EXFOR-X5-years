     ,"SAMPLE":[
	{"x4pointer":" "
	,"x4code":null
	,"x4freetext":["The Target Consists of a Stack of 8 Sheets of Hot-"
		,"Rolled Steel,0.225 in. Thick with 1 in.*4 in. Faces."]
	}
      ]
     ,"DETECTOR":[
	{"x4pointer":" "
	,"x4codes":[{"code":"GEMUC", "dict":"DETECTOR", "idict":22,"hlp":"Geiger-Mueller counter"}]
