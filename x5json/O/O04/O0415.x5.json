     ,"SAMPLE":[
	{"x4pointer":" "
	,"x4code":null
	,"x4freetext":["Targets Used in this Experiment Consisted of"
		,"Cylindrical Pilot B Plastic Scintillators 1/8 in. Thick"
		,"And 5/8 in. Diameter, Containing 91.55% Carbon by"
		,"Weight."
		,"For Each Irradiation a Target was Carefully Aligned"
		,"With and Taped Directly to the Upstream Side of"
		,"Scintillation Counters C,Which was the Same Size As"
		,"The Target."
		,"Each One of the Targets was Irradiated in the Proton"
		,"Beam For Approximately 20 Min."]
	}
      ]
     ,"DETECTOR":[
	{"x4pointer":" "
	,"x4codes":[{"code":"TELES", "dict":"DETECTOR", "idict":22,"hlp":"Counter telescope"}]
	,"x4freetext":[".A Scintillation Counter Telescope."]
	}
	,{"x4pointer":" "
	,"x4codes":[{"code":"SCIN", "dict":"DETECTOR", "idict":22,"hlp":"Scintillation detector"}]
	,"x4freetext":[".1/8-In.-Thick Plastic Scintillator."]
	}
	,{"x4pointer":" "
	,"x4codes":[{"code":"NAICR", "dict":"DETECTOR", "idict":22,"hlp":"Sodium-Iodide crystal"}]
	}
      ]
